{
  "name": "vinyl-properties",
  "version": "0.0.0",
  "description": "Get vinyl file properties in a stream",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/vinyl-properties",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "files": [
    "index.js"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/vinyl-properties/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "gulpfriendly",
    "gulp",
    "vinyl",
    "file",
    "property",
    "properties",
    "collection",
    "attribute",
    "attributes",
    "array"
  ],
  "dependencies": {
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "eslint": "^0.12.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.10.0",
    "tap-spec": "^2.2.0",
    "tape": "^3.4.0",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "requireBlocksOnNewline": true,
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
